Program.Sub.MenuClickCreatePO.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.iCnt.Declare
	V.Local.dDueDate.Declare
	V.Local.fExtension.Declare
	V.Local.sSelect.Declare
	v.Local.iSelect.Declare
	V.Local.sPO.Declare
	V.Local.sParams.Declare
	v.Local.sJob.Declare
	v.Local.sSuf.Declare
	v.Local.sSeq.Declare
	v.Local.sPart.Declare
	v.Local.sSQL.Declare
	v.Local.sDT.Declare
	v.Local.sMsg.Declare
	v.Local.sDT.Set(v.Args.DT)
	
	'Create Individual POs
	'get all selected rows
	F.Data.DataTable.Select(v.Local.sDT,"SelectRow = True",V.Local.sSelect)
	F.Intrinsic.Control.If(V.Local.sSelect,<>,"***NORETURN***")
		F.Intrinsic.String.Split(V.Local.sSelect,"*!*",V.Local.sSelect)
		F.Intrinsic.Control.For(V.Local.iCnt,0,V.Local.sSelect.UBound,1)
			v.Local.iSelect.Set(V.Local.sSelect(V.Local.iCnt))
	'they want to use vendor and cost from last PO
			f.Intrinsic.Control.CallSub(getLastPOInfo,"Part",V.DataTable.[v.Local.sDT](v.local.iSelect).PART!FieldValTrim)
	'if they have previous PO for this part
			F.Intrinsic.Control.If(v.Args.Vendor,<>,"")
				V.Local.dDueDate.Set(V.DataTable.[v.Local.sDT](v.local.iSelect).MUST_DLVR_BY_DATE!FieldValTrim)
				F.Intrinsic.Math.Mult(V.DataTable.[v.Local.sDT](v.local.iSelect).QTY_BO!FieldValTrim,v.Args.Cost,V.Local.fExtension)
				F.Data.DataTable.AddRow("175100","ID",v.local.iSelect,"AutoNumber","Y","AddComments","Y","OpenPO","Y")
				F.Data.DataTable.AddRow("175100File","ID",v.local.iSelect,"Vendor",v.Args.Vendor,"Part",V.DataTable.[v.Local.sDT](v.local.iSelect).PART!FieldValTrim,"Loc",V.DataTable.[v.Local.sDT](v.local.iSelect).LOCATION!FieldValTrim,"DueDate",V.Local.dDueDate.FormatYYYYMMDD,"Description",V.DataTable.[v.Local.sDT](v.local.iSelect).DESCRIPTION!FieldValTrim,"InvUM",V.DataTable.[v.Local.sDT](v.local.iSelect).UM_INVNEOTRY!FieldValTrim,"InvCost",V.DataTable.PJ(v.local.iSelect).Cost!FieldValTrim,"InvQty",V.DataTable.[v.Local.sDT](v.local.iSelect).QTY_BO!FieldValTrim,"POUM",V.DataTable.[v.Local.sDT](v.local.iSelect).UM_INVNEOTRY!FieldValTrim,"POCost",v.Args.Cost,"POQty",V.DataTable.[v.Local.sDT](v.local.iSelect).QTY_BO!FieldValTrim,"GLAccount","1132","Extension",V.Local.fExtension)
	'id they don't tell them
			F.Intrinsic.Control.Else
				F.Data.DataTable.SetValue(v.Local.sDT,v.local.iSelect,"SelectRow",False)
				f.Intrinsic.String.Build("Part {0} does not have previous PO!",V.DataTable.[v.Local.sDT](v.local.iSelect).PART!FieldValTrim,v.Local.sMsg)
				f.Intrinsic.UI.Msgbox(v.Local.sMsg,"Notification")
			F.Intrinsic.Control.EndIf
			
		F.Intrinsic.Control.Next(V.Local.iCnt)
		
	F.Intrinsic.Control.EndIf
	'upload PO
	F.Intrinsic.Control.If(V.Local.sSelect.UBound,>,-1)
		F.Intrinsic.Control.CallSub(175100Sync)			
	F.Intrinsic.Control.EndIf
		
	
	Gui.frmPO.lblStatus.Visible(False)

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.CallSub("Unload")
	F.Intrinsic.Control.EndTry
